    "custom_liquid_eDzkYG": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<script>\n(function(){\n  // On cible uniquement le texte \"Stock faible\" / \"Low stock\" et on n'enlève que ce libellé.\n  const TEXTS = ['stock faible','low stock'];\n  const isLow = s => TEXTS.includes((s||'').trim().toLowerCase());\n\n  function scrub(root=document){\n    // Parcourt les nœuds texte pour éviter de masquer un gros conteneur\n    const walker = document.createTreeWalker(root, NodeFilter.SHOW_TEXT, null);\n    let t;\n    while (t = walker.nextNode()){\n      if (!t.nodeValue) continue;\n      if (isLow(t.nodeValue)) {\n        const el = t.parentElement;\n        // 1) Si c'est un petit badge/label évident -> on masque juste CE petit élément\n        if (el && /(badge|inventory|stock|status)/i.test(el.className||'')) {\n          el.style.display = 'none';\n          continue;\n        }\n        // 2) Si c'est un inline simple (SPAN/SMALL/EM/STRONG/P) sans enfants -> on le masque\n        if (el && ['SPAN','SMALL','EM','STRONG','P'].includes(el.tagName) && el.childElementCount === 0) {\n          el.style.display = 'none';\n          continue;\n        }\n        // 3) Fallback ultra-sûr : on remplace UNIQUEMENT le texte, sans toucher au conteneur\n        t.nodeValue = '';\n      }\n    }\n  }\n\n  // Exécuter maintenant + après tout re-render dynamique\n  scrub();\n  new MutationObserver(() => scrub()).observe(document.documentElement, {childList:true,subtree:true,characterData:true});\n})();\n</script>"
      }
    }
  },
